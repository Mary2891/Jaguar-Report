<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="3dp">

        <ImageButton
            android:id="@+id/queja1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icosug"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_marginHorizontal="5dp"
            android:layout_marginVertical="3dp"
            >
        <TextView
            android:id="@+id/tvqueja1"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:text="Administrativa"
            android:textSize="24dp"
            android:textColor="#554b4b"/>
        <TextView
            android:id="@+id/tvdesqueja1"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:text="Descripción de la sugerencia"
            android:textSize="20dp"
            />

        </LinearLayout>

        <TextView
            android:id="@+id/fechaque1"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="19/11/2019"
            android:textSize="18dp"
            android:layout_margin="5dp"
            android:layout_gravity="center"

            />

    </LinearLayout>

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D5D2ca"
        android:layout_marginBottom="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="3dp">

        <ImageButton
            android:id="@+id/queja2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icoqueee"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_marginHorizontal="5dp"
            android:layout_marginVertical="3dp"
            >
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Mantenimiento"
                android:textSize="24dp"
                android:textColor="#554b4b"/>
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Mantenimiento"
                android:textSize="20dp"
                />

        </LinearLayout>

        <TextView
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="19/11/2019"
            android:textSize="18dp"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            />

    </LinearLayout>

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D5D2ca"
        android:layout_marginBottom="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="3dp">

        <ImageButton
            android:id="@+id/queja4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icofe"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_marginHorizontal="5dp"
            android:layout_marginVertical="3dp"
            >
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Docentes"
                android:textSize="24dp"
                android:textColor="#554b4b"/>
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Decripción de la felicita
                ción"
                android:textSize="20dp"
                />

        </LinearLayout>

        <TextView
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="19/11/2019"
            android:textSize="18dp"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            />

    </LinearLayout>

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D5D2ca"
        android:layout_marginBottom="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="3dp">

        <ImageButton
            android:id="@+id/queja5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icosug"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_marginHorizontal="5dp"
            android:layout_marginVertical="3dp"
            >
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Seguridad"
                android:textSize="24dp"
                android:textColor="#554b4b"/>
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Descripción de la sugerencia"
                android:textSize="20dp"
                />

        </LinearLayout>

        <TextView
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="19/11/2019"
            android:textSize="18dp"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            />

    </LinearLayout>

    <View
        android:id="@+id/divider6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D5D2ca"
        android:layout_marginBottom="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="3dp">

        <ImageButton
            android:id="@+id/queja6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icoqueee"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_marginHorizontal="5dp"
            android:layout_marginVertical="3dp"
            >
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Servicios escolares"
                android:textSize="24dp"
                android:textColor="#554b4b"/>
            <TextView
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="Descripción de la queja Servicios escolares"
                android:textSize="20dp"
                />

        </LinearLayout>

        <TextView
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="19/11/2019"
            android:textSize="18dp"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            />

    </LinearLayout>

    <View
        android:id="@+id/divider7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D5D2ca"
        android:layout_marginBottom="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="bottom">

        <ImageButton
            android:id="@+id/reportas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight=".01"
            android:layout_marginHorizontal="2.5dp"
            android:src="@drawable/archivo"
            android:onClick="report"
            android:layout_gravity="bottom"
            android:background="#ffffff"/>
        <ImageButton
            android:id="@+id/reporta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight=".01"
            android:layout_marginHorizontal="2.5dp"
            android:src="@drawable/usuariooo"
            android:onClick="princ"
            android:layout_gravity="bottom"
            android:padding="3dp"
            android:background="#D38A07"/>
        <ImageButton
            android:id="@+id/salir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight=".01"
            android:layout_marginHorizontal="2.5dp"
            android:src="@drawable/sall"
            android:onClick="salir"
            android:layout_gravity="bottom"
            android:background="#ffffff"/>
    </LinearLayout>

</LinearLayout>